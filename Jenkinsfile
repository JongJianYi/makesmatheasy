  agent any
  
  stages {
    
    stage("build") {
      steps{
        echo "currently building application"
      }
    }
    
     stage("test") {
      steps{
        echo "currently testing application"
      }
    }
    
     stage("deploy") {
      steps{
        echo "currently deploying application"      
      }
    }
    
    
  }
 
}
